smGain <-  function (x, gain=1) {
2 / (1 + exp(-2*gain*x)) -1
}
plotSmGain <- function (gain) {
cat ("gain=",gain, "smGain(.001, gain)/.001=", smGain(.001, gain)/.001, '\n')
x1 <- seq(-1,1, by=.01)
plot <- c (rep('sigmoid', length(x1)), rep('x=y', length(x1)))
x2   <- c (x1, x1)
y    <- c (smGain(x1, gain), x1)
smGainPDF <- data.frame (x=x2, y=y, plot=plot)
ggplot(smGainPDF, aes(x=x, y=y, colour=plot)) + geom_line()
}
plotSmGain (1.1)
